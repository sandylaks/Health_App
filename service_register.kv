                    BoxLayout:
                        orientation: 'vertical'
                        size_hint_y: None
                        height: self.minimum_height
                        padding: dp(20)
                        spacing: dp(10)
                        # signup button
                    Button:
                        id: service_register
                        text: "REGISTER"
                        size_hint: .7,None
                        height:dp(40)
                        pos_hint: {"center_x": .5, "center_y": .34}
                        background_color: 0, 0, 0, 0
                        font_name: "Roboto"
                        on_release:
                        canvas.before:
                            Color:
                                rgb: rgba(255, 0, 0, 255)
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                                radius: [5]
<HospitalContent>
    MDBoxLayout:
        orientation: 'vertical'
        size_hint_y: None
        height: self.minimum_height
        MDLabel:
            text:'Add Hospital'
        MDTextField:
            id:extra_info
            hint_text: "Hospital Name"
#                        required: True
            font_name: "Roboto"
            font_size: "14sp"
            pos_hint:{'center_x':.5}
            size_hint:.9,None
            write_tab: False
            height: "30dp"
            multiline: False
            color_mode: 'custom'
            line_color_focus: 0, 0, 0, 1
            text_color_focus:0,0,0,1
            hint_text_color:0,0,0,1
            hint_text_color_focus:0,0,0,.5
        MDTextField:
            id: State
            hint_text: "State"
            pos_hint: {'center_x': 0.5}
            on_focus: if self.focus: root.open_dropdown2(self)
            font_size: "14sp"
            size_hint:.9,None
            cursor_width: "2sp"
            readonly: True
            write_tab: False
            theme_text_color: "Custom"
            icon_right: 'chevron-down'
            color_mode: 'custom'
            line_color_focus: 0, 0, 0, 1
            text_color_focus:0,0,0,1
            hint_text_color:0,0,0,1
            hint_text_color_focus:0,0,0,.5
            icon_right_color:0,0,0,1
            icon_right_color_focus:0,0,0,.5
        MDTextField:
            id:pincode
            hint_text: "PIN Code"
#                        required: True
            font_name: "Roboto"
            font_size: "14sp"
            pos_hint:{'center_x':.5}
            size_hint:.9,None
            height: "30dp"
            multiline: False
            write_tab: False
            color_mode: 'custom'
            line_color_focus: 0, 0, 0, 1
            text_color_focus:0,0,0,1
            hint_text_color:0,0,0,1
            hint_text_color_focus:0,0,0,.5
        MDTextField:
            id:address
            hint_text: "Address"
#                        required: True
            font_name: "Roboto"
            pos_hint:{'center_x':.5}
            size_hint:.9,None
            height: "30dp"
            multiline: False
            write_tab: False
            color_mode: 'custom'
            line_color_focus: 0, 0, 0, 1
            text_color_focus:0,0,0,1
            hint_text_color:0,0,0,1
            hint_text_color_focus:0,0,0,.5

        MDTextField:
            id:extra_info2
            hint_text: "Established Year"
#                        required: True
            font_name: "Roboto"
            font_size: "14sp"
            pos_hint:{'center_x':.5}
            size_hint:.9,None
            write_tab: False
            height: "30dp"
            multiline: False
            color_mode: 'custom'
            line_color_focus: 0, 0, 0, 1
            text_color_focus:0,0,0,1
            hint_text_color:0,0,0,1
            hint_text_color_focus:0,0,0,.4
            on_focus: if self.focus: root.show_date_picker(self)
        MDBoxLayout:
            size_hint_y: None
            padding: dp(10)
            spacing: dp(20)
            orientation: 'horizontal'
            height: self.minimum_height
            MDLabel:
                text:'Upload Required Documents'
                size_hint_y:.1
                halign:'center'
                font_size: "16sp"
        GridLayout:
            id: grid
            cols: 2
            padding: dp(10)
            spacing: dp(20)
            size_hint_y: None
            height: self.minimum_height
            MDLabel:
                id: Doc_1
                text: "1.Medical Practitioner License"
                font_name: "Roboto"
                font_size: "13sp"
                pos_hint: {"center_x": .5,'center_y':.5}
                height: "15dp"
            MDLabel:
                id: file_path
                text: "None selected 1"  # Initial text when no file is selected
                italic: True
                color: 0.5, 0.5, 0.5, 1
                pos_hint: {"center_x": .8,'center_y':.4}
                size_hint_y: None
                height: "30dp"
        MDBoxLayout:
            orientation: 'horizontal'
            size_hint_y: None
            padding: dp(10)
            spacing: dp(20)
            height: self.minimum_height
            MDFlatButton:
                text: "Select File"
                size_hint:.4,.5
                on_release: root.file_manager_open('file_path')
                pos_hint: {"center_x":.3,'center_y':.5}
                background_color: 0, 0, 0, 0
                font_name: "Roboto"
                font_size: "14sp"
                background_color: 0, 0, 0, 0
                color: rgba(1, 1, 1, 255)
                canvas.before:
                    Color:
                        rgb: rgba(255, 0, 0, 255)
                    Line:
                        width: 1.2
                        rounded_rectangle: self.x, self.y, self.width, self.height, 5, 5, 5, 5, 100
            MDFlatButton:
                id: upload
                text: "Upload"
                size_hint:.4,.5
                pos_hint: {"center_x":.3,'center_y':.5}
                background_color: 0, 0, 0, 0
                font_name: "Roboto"
                font_size: "14sp"
                background_color: 0, 0, 0, 0
                color: rgba(1, 1, 1, 255)
                on_release: root.upload_file("file_path")
                canvas.before:
                    Color:
                        rgb: rgba(255, 0, 0, 255)
                    Line:
                        width: 1.2
                        rounded_rectangle: self.x, self.y, self.width, self.height, 5, 5, 5, 5, 100
        GridLayout:
            id: grid
            cols: 2
            padding: dp(10)
            spacing: dp(20)
            size_hint_y: None
            height: self.minimum_height
            MDLabel:
                id: Doc_2
                text: "2.Building Permit and Licenses"
                font_name: "Roboto"
                font_size: "13sp"
                pos_hint: {"center_x": .5,'center_y':.4}
                height: "15dp"
                required:True

            MDLabel:
                id: file_path2
                text: "None selected 2"  # Initial text when no file is selected
                italic: True
                color: 0.5, 0.5, 0.5, 1
                pos_hint: {"center_x": .8,'center_y':.4}
                size_hint_y: None
                height: "30dp"
        MDBoxLayout:
            orientation: 'horizontal'
            size_hint_y: None
            padding: dp(10)
            spacing: dp(20)
            height: self.minimum_height
            MDFlatButton:
                text: "Select File"
                size_hint:.4,.5
                on_release: root.file_manager_open('file_path2')
                pos_hint: {"center_x":.3,'center_y':.5}
                background_color: 0, 0, 0, 0
                font_name: "Roboto"
                font_size: "14sp"
                background_color: 0, 0, 0, 0
                color: rgba(1, 1, 1, 255)
                canvas.before:
                    Color:
                        rgb: rgba(255, 0, 0, 255)
                    Line:
                        width: 1.2
                        rounded_rectangle: self.x, self.y, self.width, self.height, 5, 5, 5, 5, 100
            MDFlatButton:
                id: upload
                text: "Upload"
                size_hint:.4,.5
                pos_hint: {"center_x":.3,'center_y':.5}
                background_color: 0, 0, 0, 0
                font_name: "Roboto"
                font_size: "14sp"
                background_color: 0, 0, 0, 0
                color: rgba(1, 1, 1, 255)
                on_release: root.upload_file("file_path2")
                canvas.before:
                    Color:
                        rgb: rgba(255, 0, 0, 255)
                    Line:
                        width: 1.2
                        rounded_rectangle: self.x, self.y, self.width, self.height, 5, 5, 5, 5, 100




